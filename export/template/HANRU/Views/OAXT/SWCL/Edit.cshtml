                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.DJRGH) </td> <!--登记人工号-->
                <td>
                    @Html.TextBoxFor(m => m.DJRGH, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.DJRGH)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SWRQ) </td> <!--收文日期-->
                <td>
                    @Html.TextBoxFor(m => m.SWRQ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.SWRQ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.LWDW) </td> <!--来文单位-->
                <td>
                    @Html.TextBoxFor(m => m.LWDW, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.LWDW)
                </td>
            </tr>

            <tr>
