@model HanRuEdu.LDAL.EDU_ZZFC_08_A01_SSCW
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--床位ID-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SSLID) </td> <!--宿舍楼ID-->
                <td>
                    @Html.DropDownListFor(m => m.SSLID, ViewBag.ZZFC_02Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SSLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SSID) </td> <!--宿舍ID-->
                <td>
                    @Html.DropDownListFor(m => m.SSID, ViewBag.ZZFC_08Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SSID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.CWM) </td> <!--床位名-->
                <td>
                    @Html.TextBoxFor(m => m.CWM, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.CWM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.BZ) </td> <!--备注-->
                <td>
                    @Html.TextBoxFor(m => m.BZ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.BZ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SFRZ) </td> <!--是否入住-->
                <td>
                    @Html.DropDownListFor(m => m.SFRZ, ViewBag.JY_SFBZLst as SelectList)
                    @Html.ValidationMessageFor(m => m.SFRZ)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/xtgl/sscw/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
