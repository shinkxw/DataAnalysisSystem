@model HanRuEdu.LDAL.EDU_ZZJX_02_01_ZZNJ
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.NJDM) </td> <!--年级代码-->
                <td>
                    @Html.TextBoxFor(m => m.NJDM, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.NJDM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校名-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.NJMC) </td> <!--年级名称-->
                <td>
                    @Html.TextBoxFor(m => m.NJMC, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.NJMC)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SSNF) </td> <!--所属年份-->
                <td>
                    @Html.TextBoxFor(m => m.SSNF, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.SSNF)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.NJZT) </td> <!--年级状态-->
                <td>
                    @Html.DropDownListFor(m => m.NJZT, ViewBag.JY_SFBZLst as SelectList)
                    @Html.ValidationMessageFor(m => m.NJZT)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/xtgl/zznj/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
