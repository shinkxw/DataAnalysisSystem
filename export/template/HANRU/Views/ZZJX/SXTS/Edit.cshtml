            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.QYJY) </td> <!--企业建议-->
                <td>
                    @Html.TextBoxFor(m => m.QYJY, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.QYJY)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/sxts/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
