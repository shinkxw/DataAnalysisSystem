@model HanRuEdu.LDAL.EDU_ZZJX_07_A03_XXSXZY
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/xtgl/XXSXZY/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--实习周/月ID-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="SXJHID" width="50" sortable="true">@Html.LabelFor(m => m.SXJHID)</th><!--实习计划ID-->
            <th field="SXJDID" width="50" sortable="true">@Html.LabelFor(m => m.SXJDID)</th><!--实习阶段ID-->
            <th field="MC" width="50" sortable="true">@Html.LabelFor(m => m.MC)</th><!--名称-->
            <th field="KSSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.KSSJ)</th><!--开始时间-->
            <th field="JSSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.JSSJ)</th><!--结束时间-->
            <th field="LX" width="50" sortable="true">@Html.LabelFor(m => m.LX)</th><!--类型-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/xtgl/xxsxzy/create");
    ViewData["index_edit"] = Url.Content("~/xtgl/xxsxzy/edit");
    //ViewData["index_detail"] = Url.Content("~/xtgl/xxsxzy/details");
    //ViewData["index_del"] = Url.Content("~/xtgl/xxsxzy/delete");
    ViewData["index_multi_del"] = Url.Content("~/xtgl/xxsxzy/delete");
    ViewData["index_id_name"] = "ID";
