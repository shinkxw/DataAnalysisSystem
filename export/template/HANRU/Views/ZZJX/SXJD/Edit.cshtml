                    @Html.DropDownListFor(m => m.XSXXID, ViewBag.ZZXS_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.XSXXID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JDNR) </td> <!--鉴定内容-->
                <td>
                    @Html.TextBoxFor(m => m.JDNR, new { @class = "easyui-validatebox", style = "width:150px; " })
