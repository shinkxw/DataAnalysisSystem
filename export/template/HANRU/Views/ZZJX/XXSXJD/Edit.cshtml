@model HanRuEdu.LDAL.EDU_ZZJX_07_A02_XXSXJD
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--实习阶段ID-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SXJHID) </td> <!--实习计划ID-->
                <td>
                    @Html.DropDownListFor(m => m.SXJHID, ViewBag.ZZJX_07Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SXJHID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JDM) </td> <!--阶段名-->
                <td>
                    @Html.TextBoxFor(m => m.JDM, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.JDM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JDKSSJ) </td> <!--阶段开始时间-->
                <td>
                    @Html.TextBoxFor(m => m.JDKSSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.JDKSSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JDJSSJ) </td> <!--阶段结束时间-->
                <td>
                    @Html.TextBoxFor(m => m.JDJSSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.JDJSSJ)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/xtgl/xxsxjd/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
