@model HanRuEdu.LDAL.EDU_ZXDY_04_A02_ZZRYAP
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/jpxt/ZZRYAP/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--值周人员安排表ID-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="XQID" width="50" sortable="true">@Html.LabelFor(m => m.XQID)</th><!--学期ID-->
            <th field="ZCID" width="50" sortable="true">@Html.LabelFor(m => m.ZCID)</th><!--周次ID-->
            <th field="KSRQ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.KSRQ)</th><!--开始日期-->
            <th field="JSRQ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.JSRQ)</th><!--结束日期-->
            <th field="ZZBJ" width="50" sortable="true">@Html.LabelFor(m => m.ZZBJ)</th><!--值周班级班号-->
            <th field="ZZID" width="50" sortable="true">@Html.LabelFor(m => m.ZZID)</th><!--组长ID-->
            <th field="ZYIDLB" width="50" sortable="true">@Html.LabelFor(m => m.ZYIDLB)</th><!--组员ID列表-->
            <th field="ZYMCLB" width="50" sortable="true">@Html.LabelFor(m => m.ZYMCLB)</th><!--组员名称列表-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/jpxt/zzryap/create");
    ViewData["index_edit"] = Url.Content("~/jpxt/zzryap/edit");
    //ViewData["index_detail"] = Url.Content("~/jpxt/zzryap/details");
