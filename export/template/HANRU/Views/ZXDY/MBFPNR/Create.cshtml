@model HanRuEdu.LDAL.EDU_ZXDY_04_A07_MBFPNR
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="添加" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">添加</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--模板内容表ID-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.MBID) </td> <!--模板表ID-->
                <td>
                    @Html.TextBoxFor(m => m.MBID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.MBID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.GZR) </td> <!--工作日-->
                <td>
                    @Html.TextBoxFor(m => m.GZR, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.GZR)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.GWID) </td> <!--岗位ID-->
                <td>
                    @Html.TextBoxFor(m => m.GWID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.GWID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.DYRYH) </td> <!--对应人员号-->
                <td>
                    @Html.TextBoxFor(m => m.DYRYH, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.DYRYH)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/mbfpnr/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
