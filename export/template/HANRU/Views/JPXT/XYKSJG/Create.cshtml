@model HanRuEdu.LDAL.EDU_JPXT_03_02_XYKSJG
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="添加" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">添加</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--编号-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.XYID) </td> <!--学员ID-->
                <td>
                    @Html.DropDownListFor(m => m.XYID, ViewBag.JPXT_03Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.XYID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KMKSID) </td> <!--科目考试ID-->
                <td>
                    @Html.DropDownListFor(m => m.KMKSID, ViewBag.JPXT_04Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.KMKSID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KSKMID) </td> <!--考试科目ID-->
                <td>
                    @Html.TextBoxFor(m => m.KSKMID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.KSKMID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KSJG) </td> <!--考试结果-->
                <td>
                    @Html.TextBoxFor(m => m.KSJG, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.KSJG)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/xyksjg/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
