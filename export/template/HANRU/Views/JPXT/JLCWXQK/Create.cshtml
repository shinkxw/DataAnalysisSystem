@model HanRuEdu.LDAL.EDU_JPXT_01_03_JLCWXQK
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="添加" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">添加</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--编号-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JLCID) </td> <!--教练车ID-->
                <td>
                    @Html.DropDownListFor(m => m.JLCID, ViewBag.JPXT_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.JLCID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.GZXX) </td> <!--故障现象-->
                <td>
                    @Html.TextBoxFor(m => m.GZXX, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.GZXX)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SXSJ) </td> <!--送修时间-->
                <td>
                    @Html.TextBoxFor(m => m.SXSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.SXSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.WCSJ) </td> <!--完成时间-->
                <td>
                    @Html.TextBoxFor(m => m.WCSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.WCSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.WXJG) </td> <!--维修结果-->
                <td>
                    @Html.TextBoxFor(m => m.WXJG, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.WXJG)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.BZ) </td> <!--备注-->
                <td>
                    @Html.TextBoxFor(m => m.BZ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.BZ)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/jlcwxqk/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
