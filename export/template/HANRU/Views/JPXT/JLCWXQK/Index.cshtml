@model HanRuEdu.LDAL.EDU_JPXT_01_03_JLCWXQK
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/xtgl/JLCWXQK/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--编号-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="JLCID" width="50" sortable="true">@Html.LabelFor(m => m.JLCID)</th><!--教练车ID-->
            <th field="GZXX" width="50" sortable="true">@Html.LabelFor(m => m.GZXX)</th><!--故障现象-->
            <th field="SXSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.SXSJ)</th><!--送修时间-->
            <th field="WCSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.WCSJ)</th><!--完成时间-->
            <th field="WXJG" width="50" sortable="true">@Html.LabelFor(m => m.WXJG)</th><!--维修结果-->
            <th field="BZ" width="50" sortable="true">@Html.LabelFor(m => m.BZ)</th><!--备注-->
        </tr>
    </thead>
</table>
