@model HanRuEdu.LDAL.EDU_ELE_01_USER
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/jpxt/USER/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="LOGINNAME" width="50" sortable="true">@Html.LabelFor(m => m.LOGINNAME)</th><!--用户名-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="APPID" width="50" sortable="true">@Html.LabelFor(m => m.APPID)</th><!--应用ID-->
            <th field="PWD" width="50" sortable="true">@Html.LabelFor(m => m.PWD)</th><!--密码-->
            <th field="STATUS" width="50" sortable="true">@Html.LabelFor(m => m.STATUS)</th><!--用户状态-->
            <th field="USERTYPE" width="50" sortable="true">@Html.LabelFor(m => m.USERTYPE)</th><!--用户类别-->
            <th field="USERID" width="50" sortable="true">@Html.LabelFor(m => m.USERID)</th><!--用户ID-->
            <th field="ROLEIDLst" width="50" sortable="true">@Html.LabelFor(m => m.ROLEIDLst)</th><!--用户角色-->
            <th field="XM" width="50" sortable="true">@Html.LabelFor(m => m.XM)</th><!--姓名-->
            <th field="XB" width="50" sortable="true">@Html.LabelFor(m => m.XB)</th><!--性别-->
            <th field="QQ" width="50" sortable="true">@Html.LabelFor(m => m.QQ)</th><!--QQ-->
            <th field="DZYJ" width="50" sortable="true">@Html.LabelFor(m => m.DZYJ)</th><!--电子邮件-->
            <th field="LXDH" width="50" sortable="true">@Html.LabelFor(m => m.LXDH)</th><!--联系电话-->
            <th field="ZJDLSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.ZJDLSJ)</th><!--最近一次登录时间-->
            <th field="DLCGCS" width="50" sortable="true">@Html.LabelFor(m => m.DLCGCS)</th><!--登录成功次数-->
            <th field="YHCJSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.YHCJSJ)</th><!--用户创建时间-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/jpxt/user/create");
    ViewData["index_edit"] = Url.Content("~/jpxt/user/edit");
    //ViewData["index_detail"] = Url.Content("~/jpxt/user/details");
    //ViewData["index_del"] = Url.Content("~/jpxt/user/delete");
    ViewData["index_multi_del"] = Url.Content("~/jpxt/user/delete");
    ViewData["index_id_name"] = "ID";
    //ViewData["index_outdata"] = Url.Content("~/zsgl/xsdj/OutExcel");
    //ViewData["index_indata"] = Url.Content("~/zsgl/xsdj/ImportData");
}
@Html.Partial("~/views/shared/indexToolBarPage.cshtml", this.ViewData)
