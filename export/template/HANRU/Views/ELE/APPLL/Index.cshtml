@model HanRuEdu.LDAL.EDU_ELE_03_APPLL
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/jpxt/APPLL/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--编号-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校名-->
            <th field="APPID" width="50" sortable="true">@Html.LabelFor(m => m.APPID)</th><!--应用ID-->
            <th field="YHID" width="50" sortable="true">@Html.LabelFor(m => m.YHID)</th><!--操作用户-->
            <th field="CZSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.CZSJ)</th><!--操作时间-->
            <th field="CZYY" width="50" sortable="true">@Html.LabelFor(m => m.CZYY)</th><!--操作应用-->
            <th field="CZBM" width="50" sortable="true">@Html.LabelFor(m => m.CZBM)</th><!--操作表名-->
            <th field="CZXX" width="50" sortable="true">@Html.LabelFor(m => m.CZXX)</th><!--操作信息-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/jpxt/appll/create");
    ViewData["index_edit"] = Url.Content("~/jpxt/appll/edit");
    //ViewData["index_detail"] = Url.Content("~/jpxt/appll/details");
    //ViewData["index_del"] = Url.Content("~/jpxt/appll/delete");
    ViewData["index_multi_del"] = Url.Content("~/jpxt/appll/delete");
    ViewData["index_id_name"] = "ID";
    //ViewData["index_outdata"] = Url.Content("~/zsgl/xsdj/OutExcel");
    //ViewData["index_indata"] = Url.Content("~/zsgl/xsdj/ImportData");
}
@Html.Partial("~/views/shared/indexToolBarPage.cshtml", this.ViewData)
