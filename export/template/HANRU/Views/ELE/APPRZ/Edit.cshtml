@model HanRuEdu.LDAL.EDU_ELE_03_APPRZ
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--编号-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校名-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.APPID) </td> <!--应用ID-->
                <td>
                    @Html.TextBoxFor(m => m.APPID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.APPID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.RZLX) </td> <!--日志类型-->
                <td>
                    @Html.TextBoxFor(m => m.RZLX, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.RZLX)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.YHID) </td> <!--操作用户-->
                <td>
                    @Html.DropDownListFor(m => m.YHID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.YHID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.CZSJ) </td> <!--操作时间-->
                <td>
                    @Html.TextBoxFor(m => m.CZSJ, new { @class = "easyui-datetimebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.CZSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.CZYY) </td> <!--操作应用-->
                <td>
                    @Html.TextBoxFor(m => m.CZYY, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.CZYY)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.CZLX) </td> <!--操作类型-->
                <td>
                    @Html.TextBoxFor(m => m.CZLX, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.CZLX)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.CZJTXX) </td> <!--操作具体信息-->
                <td>
                    @Html.TextBoxFor(m => m.CZJTXX, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.CZJTXX)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/apprz/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
