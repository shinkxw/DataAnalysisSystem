@model HanRuEdu.LDAL.EDU_ELE_05_XL
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/jpxt/XL/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--编号-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="NAME" width="50" sortable="true">@Html.LabelFor(m => m.NAME)</th><!--校历名称-->
            <th field="STARTDAY" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.STARTDAY)</th><!--开始日期-->
            <th field="ENDDAY" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.ENDDAY)</th><!--结束日期-->
            <th field="XNID" width="50" sortable="true">@Html.LabelFor(m => m.XNID)</th><!--学年ID-->
            <th field="XQID" width="50" sortable="true">@Html.LabelFor(m => m.XQID)</th><!--学期ID-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/jpxt/xl/create");
    ViewData["index_edit"] = Url.Content("~/jpxt/xl/edit");
    //ViewData["index_detail"] = Url.Content("~/jpxt/xl/details");
    //ViewData["index_del"] = Url.Content("~/jpxt/xl/delete");
    ViewData["index_multi_del"] = Url.Content("~/jpxt/xl/delete");
    ViewData["index_id_name"] = "ID";
    //ViewData["index_outdata"] = Url.Content("~/zsgl/xsdj/OutExcel");
    //ViewData["index_indata"] = Url.Content("~/zsgl/xsdj/ImportData");
}
@Html.Partial("~/views/shared/indexToolBarPage.cshtml", this.ViewData)
