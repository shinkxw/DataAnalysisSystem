@model HanRuEdu.LDAL.EDU_ZZJG_04_A01_JBDJ
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--加班登记表ID-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.DropDownListFor(m => m.SCHOOLID, ViewBag.ELE_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JZGJBSJID) </td> <!--教职工ID-->
                <td>
                    @Html.DropDownListFor(m => m.JZGJBSJID, ViewBag.ZZJG_01Lst as SelectList)
                    @Html.ValidationMessageFor(m => m.JZGJBSJID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KSSJ) </td> <!--开始时间-->
                <td>
                    @Html.TextBoxFor(m => m.KSSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.KSSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JSSJ) </td> <!--结束时间-->
                <td>
                    @Html.TextBoxFor(m => m.JSSJ, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.JSSJ)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JBLX) </td> <!--加班类型-->
                <td>
                    @Html.DropDownListFor(m => m.JBLX, ViewBag.ZJ_JBLXLst as SelectList)
                    @Html.ValidationMessageFor(m => m.JBLX)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/xtgl/jbdj/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
