@model HanRuEdu.LDAL.EDU_ZZJG_04_A01_JBDJ
<table id="dg" title="  " class="easyui-datagrid" style="width:900px;height:500px"
            data-options="singleSelect:false,collapsible:true,  url:'@Url.Content("~/xtgl/JBDJ/index_jsonstr")',
            toolbar:'#toolbar', remoteSort:true,pagination:true, rownumbers:true, fitColumns:true,multiSort:true" >
    <thead>
        <tr>
            <!--<th data-options="field:'ck',checkbox:true"></th>-->
            <th field="ID" width="50" sortable="true">@Html.LabelFor(m => m.ID)</th><!--加班登记表ID-->
            <th field="SCHOOLID" width="50" sortable="true">@Html.LabelFor(m => m.SCHOOLID)</th><!--学校ID-->
            <th field="JZGJBSJID" width="50" sortable="true">@Html.LabelFor(m => m.JZGJBSJID)</th><!--教职工ID-->
            <th field="KSSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.KSSJ)</th><!--开始时间-->
            <th field="JSSJ" width="50" formatter="formatDatebox" sortable="true">@Html.LabelFor(m => m.JSSJ)</th><!--结束时间-->
            <th field="JBLX" width="50" sortable="true">@Html.LabelFor(m => m.JBLX)</th><!--加班类型-->
        </tr>
    </thead>
</table>
@{
    ViewData["index_create"] = Url.Content("~/xtgl/jbdj/create");
    ViewData["index_edit"] = Url.Content("~/xtgl/jbdj/edit");
    //ViewData["index_detail"] = Url.Content("~/xtgl/jbdj/details");
    //ViewData["index_del"] = Url.Content("~/xtgl/jbdj/delete");
    ViewData["index_multi_del"] = Url.Content("~/xtgl/jbdj/delete");
    ViewData["index_id_name"] = "ID";
