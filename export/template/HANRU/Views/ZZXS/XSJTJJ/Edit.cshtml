                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JXDDBX) </td> <!--就学地低保线-->
                <td>
                    @Html.TextBoxFor(m => m.JXDDBX, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.JXDDBX)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/jpxt/xsjtjj/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
