                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.JTLBM) </td> <!--家庭类别码-->
                <td>
                    @Html.DropDownListFor(m => m.JTLBM, ViewBag.JY_JTLBLst as SelectList)
                    @Html.ValidationMessageFor(m => m.JTLBM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KNYYM) </td> <!--困难原因码-->
