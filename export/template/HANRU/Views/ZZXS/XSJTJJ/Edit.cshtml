                    @Html.DropDownListFor(m => m.KNYYM, ViewBag.JY_KNYYLst as SelectList)
                    @Html.ValidationMessageFor(m => m.KNYYM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.KNCDM) </td> <!--困难程度码-->
                <td>
                    @Html.DropDownListFor(m => m.KNCDM, ViewBag.JY_KNCDLst as SelectList)
                    @Html.ValidationMessageFor(m => m.KNCDM)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SFDB) </td> <!--是否低保-->
                <td>
                    @Html.DropDownListFor(m => m.SFDB, ViewBag.JY_SFBZLst as SelectList)
                    @Html.ValidationMessageFor(m => m.SFDB)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.RXQHKLBM) </td> <!--入学前户口类别码-->
                <td>
                    @Html.DropDownListFor(m => m.RXQHKLBM, ViewBag.GB_HKLBLst as SelectList)
