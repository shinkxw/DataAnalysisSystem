@model HanRuEdu.LDAL.EDU_ZZBG_13_A01_DWSJ
@using (Html.BeginForm())
{
    @Html.Partial("SingleZTree")
    <div id="dlg" class="easyui-panel" title="编辑" style="width: 900px; height: 500px; padding: 10px 20px">
        <center>  <h1 ><span style="font-size:smaller;">编辑</span></h1></center>

        <table class="admintable">
            <tr>
                <td width="50%"></td>
                <td width="50%"></td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.ID) </td> <!--单位ID-->
                <td>
                    @Html.TextBoxFor(m => m.ID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.ID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SCHOOLID) </td> <!--学校ID-->
                <td>
                    @Html.TextBoxFor(m => m.SCHOOLID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.SCHOOLID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.SJDWID) </td> <!--上级单位ID-->
                <td>
                    @Html.TextBoxFor(m => m.SJDWID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.SJDWID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.DWFZRID) </td> <!--单位负责人ID-->
                <td>
                    @Html.TextBoxFor(m => m.DWFZRID, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.DWFZRID)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.DWMC) </td> <!--单位名称-->
                <td>
                    @Html.TextBoxFor(m => m.DWMC, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.DWMC)
                </td>
            </tr>

            <tr>
                <td> @Html.LabelFor(m => m.DWCYIDLB) </td> <!--单位成员ID列表-->
                <td>
                    @Html.TextBoxFor(m => m.DWCYIDLB, new { @class = "easyui-validatebox", style = "width:150px; " })
                    @Html.ValidationMessageFor(m => m.DWCYIDLB)
                </td>
            </tr>

        </table>
        <br />
        @{ ViewData["ce_cancel"] = Url.Content("~/xtgl/dwsj/index");}
        @Html.Partial("~/views/shared/CreateEditToolBarPage.cshtml", this.ViewData)
    </div>
}
